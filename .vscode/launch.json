{
  "configurations": [
    {
      "name": "TRE API",
      "type": "python",
      "module": "uvicorn",
      "request": "launch",
      "args": [
        "main:app",
        "--reload",
        "--host",
        "::",
        "--port",
        "8000"
      ],
      "justMyCode": false,
      "console": "integratedTerminal",
      "envFile": "${workspaceFolder}/templates/core/tre.env",
      "preLaunchTask": "Copy_env_file_for_debug",
      "cwd": "${workspaceFolder}/api_app"
    }
  ]
}
